<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Plant"
		class="sapUiPopupWithPadding"
	
		items="{path:'CmnModel>/plant',sorter:{path:'name1'}}"
		liveChange="_handleValueHelpSearch_Plant"
		confirm="_handleValueHelpClose_Plant"
		cancel="_handleValueHelpClose_Plant">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{CmnModel>werks}"
			description="{CmnModel>name1}"/>
	</SelectDialog>
</core:FragmentDefinition>