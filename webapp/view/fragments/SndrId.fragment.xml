<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Sender Id"
		class="sapUiPopupWithPadding"
		items="{path : 'IdModel>/results',sorter : {path:'sender_id',ascending: true}}"
		liveChange="_handleValueHelpSearch"
		confirm="_handleValueHelpClose_Port">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{IdModel>sender_id}"
			description="{IdModel>chacode}"
			/>
	</SelectDialog>
</core:FragmentDefinition>