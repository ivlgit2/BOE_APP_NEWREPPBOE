<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Country"
		class="sapUiPopupWithPadding"
		items="{CmnModel>/I_Country}"
		liveChange="_handleValueHelpSearch_Ctry"
		confirm="_handleValueHelpClose_Ctry"
		cancel="_handleValueHelpClose_Ctry">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{CmnModel>Country}"
			description="{CmnModel>Country_Text}"/>
	</SelectDialog>
</core:FragmentDefinition>